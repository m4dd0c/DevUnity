  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pretty:check": "prettier --check --config ./.prettierrc ./",
    "pretty:checkMe": "prettier --check --config ./.prettierrc ./global.d.ts",
    "pretty:fix": "prettier --write --config ./.prettierrc ./ && echo Run `npm run lint:check` to check linting.",
    "pretty:fixMe": "prettier --write --config ./.prettierrc ./global.d.ts && echo Run `npm run lint:check` to check linting.",
    "lint:check": "eslint . --no-fix && echo Run `npm run lint:fix` to fix them.",
    "lint:checkMe": "eslint ./global.d.ts --no-fix && echo Run `npm run lint:fix` to fix them.",
    "lint:fix": "eslint . --fix && echo Run `npm run pretty:check` to check formatting.",
    "lint:fixMe": "eslint ./global.d.ts --fix && echo Run `npm run pretty:check` to check formatting."
  },
